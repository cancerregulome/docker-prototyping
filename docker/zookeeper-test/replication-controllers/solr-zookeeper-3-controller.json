{
	"kind": "ReplicationController",
	"apiVersion": "v1beta3",
	"metadata": {
		"name": "solr-zookeeper-controller-3",
		"labels": {
			"name": "solr-zookeeper-controller"
		}
	},
	"spec": {
		"replicas": 1,
		"selector": {
			"name": "solr-zookeeper-pod-3"
		},
		"template": {
			"metadata": {
				"labels": {
					"name": "solr-zookeeper-pod-3",
					"app": "solr-zookeeper"
				}
			},
			"spec": {
				"restartPolicy": "Always",
				"containers": [
					{
						"name": "solr-zookeeper-3",
						"image": "gcr.io/isb-cgc/solr-zookeeper-3", 
						"ports": [
							{
								"containerPort": 2181,
								"protocol": "TCP"
							},
							{
								"containerPort": 2888,
								"protocol": "TCP"
							},
							{
								"containerPort": 3888,
								"prototcol": "TCP"
							}
						],
						"command": ["java", "-cp", "/zookeeper-3.4.6/zookeeper-3.4.6.jar:/zookeeper-3.4.6/lib/slf4j-api-1.6.1.jar:/zookeeper-3.4.6/lib/slf4j-log4j12-1.6.1.jar:/zookeeper-3.4.6/lib/log4j-1.2.16.jar:/zookeeper-3.4.6/conf", "org.apache.zookeeper.server.quorum.QuorumPeerMain", "/zookeeper-3.4.6/conf/zoo.cfg"]
					} ]
			}
		}
	}
}
