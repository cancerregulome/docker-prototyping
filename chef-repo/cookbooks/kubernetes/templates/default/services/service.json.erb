{
	"kind": "Service",
	"apiVersion": "v1beta3",
	"metadata": {
		"name": "<%= @service_name %>",
		"labels": {
			"name": "<%= @service_label_name %>"
		}
	},
	"spec": {
		"ports": [
		<% @service_ports.each do |port| %>
		{
			"port": "<%= port[:port] %>",
			"targetPort": "<%= port[:targetPort] %>",
			"protocol": "<%= port[:protocol] %>"
		}<% if port != @service_ports.last %>,<% end %>
		<% end %>
		],
		"selector": {
			"name": "<%= @service_selector %>"
		}
	}
}
